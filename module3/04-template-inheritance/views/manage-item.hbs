{{#extends "base"}}
  {{#block "content"}}
    <div class="container">
      {{#each allFood}}
        <div class="card mb-3" style="width: 18rem;">
          <img src="data:image/png;base64," class="card-img-top" alt="..." />
          <div class="card-body">
            <h5 class="card-title">{{this.foodName}}</h5>
            <p class="card-text">
                Calories: {{this.foodCalories}}
            </p>
            <a href="edit-item" data-id="{{this.id}}" class="btn btn-primary">Edit</a>
            <a href="delete-item" data-id="{{this.id}}" class="btn btn-danger">Delete</a>
          </div>
        </div>
      {{/each}}
    </div><!-- end of container-->
  {{/block}}
{{/extends}}